{
    "name": "tpp",
    "displayName": "TPP",
    "description": "Desenvolvimento de um highlight syntax da linguagem T++, utilizada na disciplina de Compiladores para o VsCode.",
    "version": "0.0.1",
    "engines": {
        "vscode": "^1.61.0"
    },
    "categories": [
        "Programming Languages"
    ],
    "contributes": {
        "languages": [{
            "id": "tpp",
            "aliases": ["TPP", "tpp"],
            "extensions": [".tpp"],
            "configuration": "./language-configuration.json"
        }],
        "grammars": [{
            "language": "tpp",
            "scopeName": "source.tpp",
            "path": "./syntaxes/tpp.tmLanguage.json"
        }]
    }
}